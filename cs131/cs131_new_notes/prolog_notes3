app([], L, L).
app([H|T], L, [H|A]) :- app(T, L, A).

rev([],[]).
rev([H|T],R) :- rev(T, L), app(L,[H], R)


/* Query: rev([1],L) */

My Tree:
                                    [rev([1],L)]
                                          | 
                                          | 42 , L=R
                                          |
                                   [rev([1|[]], R)]
                                          |
                                          |
                                          |
                               [rev([],L), app(L, [1], R)]
                                   /            | 
                                  /40           | 37
                                 /              | 
                               L=[]         [app([],[1],R)]
                                                 |
                                                 |
                                                 |
                                               R =[1]

Class Tree:
                                      [rev([1],L)]
                                     /     |
                                 40 /   41 | H=1
                                   /       | T=[]
                                  No       | L=R
                                  		   |
                               [rev([],L'), app(L', [1],R)]
    								/               \
    							40 /                 \
    						L'=[] /                   \
    							 /					  No
    					 [app([],[1],R)]
    					   /    |    \
    				   37 /     |     \ 38
    				     /	    |      \
    				   []      R=[1]    No


/*Query: rev(L, [1])*/

                    [rev(L,[1])]
                       /     \
                   40 /       \ 41
                     No        \ L= [H|T], R=[1]
                                \ 
                                 \
                    [rev(T,L'), app(L', [H], [1])]
                           /       \	
                  T=[] 40 /         \ 41  T=[H0|T0]
                  L'=[]  /           \    L' = R0
                        /             \	
              [app([],[H],[1])]        \
                    |	  |38			\
            L=[H] 37|	  |				 \
